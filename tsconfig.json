{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "lib": ["es6", "es2015"],
      "types": ["node"],
      "declaration": true,
      "outDir": "build",
      "baseUrl": "build",
      "strict": true
    },
    // "include": ["src/**/*"],
    "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "src/**/*"
    ],
  }